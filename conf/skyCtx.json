{
  "init": {
    "continuousRendering": true,
    "coordinateSystem": {
      "geoideName": "Equatorial"
    },
    "navigation": {
      "initTarget": [
        85.2500,
        -2.4608
      ],
      "initFov": 20,
      "inertia": true,
      "minFov": 0.001,
      "zoomFactor": 0,
      "mouse": {
        "zoomOnDblClick": true
      }
